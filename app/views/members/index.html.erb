<h1><%= basic_link_for @council %>: <%= @title %></h1>
<p><%= params[:include_ex_members] ? "<strong>Former members in grey</strong> (#{link_to('Restrict to current members', params.except(:include_ex_members))})" : link_to("Include former members", params.merge(:include_ex_members => true)) %></p>
<%- if @council -%>
  <%= list_all @members %>
<%- else -%>
  <ul id='members'>
    <%= render :partial => @members %>
  </ul>
<%- end -%>
<%= will_paginate @members unless @council %>
