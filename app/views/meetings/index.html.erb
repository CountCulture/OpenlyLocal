<h1><%= link_for @council %> <%= @title %></h1>
<p><%= link_to_calendar %> :: <%= params[:include_past] ? link_to("restrict to forthcoming meetings", params.except(:include_past)) : link_to("include meetings in the past", params.merge(:include_past => true)) %></p>
<p class="extra_info"><%= link_for @council %> :: <%= link_to h("#{@council.title} committees"), committees_path(:council_id => @council.id) %></p>
<div id="meetings">
  <%= list_all @meetings %>
</div>

<%= render :partial => '/shared/api_info' %>