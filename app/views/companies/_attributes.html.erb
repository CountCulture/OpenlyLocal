<%= attribute_tag :company_number, link_to(attributes.company_number, attributes.opencorporates_url, :class => 'external', :title => "Free and open company data on #{attributes.title}") if attributes.company_number? %>
<%= attribute_tag :vat_number, attributes.vat_number %>
<%= attribute_tag :website, link_to(attributes.url, attributes.url) if attributes.url? %>
<%= attribute_tag :wikipedia_url, link_to(URI.unescape(attributes.wikipedia_url), attributes.wikipedia_url, :class => 'external') if attributes.wikipedia_url? %>
