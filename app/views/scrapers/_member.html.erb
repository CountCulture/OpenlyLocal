<%- div_for member, :class => class_for_result(member) do -%>
<p><%= flash_for_result(member) %></p>
<%= error_messages_for :member, :object => member %>
  <h4><%= member.full_name %> <em><%= link_to "Council page", member.url, :class => "external" %></em></h4>
  <p><strong>Party</strong> <%= member.party %></p>
  <p><strong>Email</strong> <%= member.email %></p>
<%- end -%>