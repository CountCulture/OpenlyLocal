<%- div_for scraped_object, :class => "#{scraped_object.status} #{scraped_object.base_object_klass.downcase}" do -%>
  <div><%= flash_for_result(scraped_object) %></div>
  <%= error_messages_for :scraped_object, :object => scraped_object %>
    <h4><%= scraped_object.title %> <em><%= link_to("Council page", scraped_object.url, :class => "external", :only_path => false) if scraped_object.respond_to?(:url)&&!scraped_object.url.blank? %></em></h4>
    <%= changed_attributes_list(scraped_object) %>
<%- end -%>